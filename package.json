{
  "name": "www-app",
  "version": "1.0.0",
  "description": "The server half of www.stephen-cresswell.net",
  "main": "server/build/index.js",
  "scripts": {
    "start-server": "nodemon server/index.js --exec babel-node",
    "start-client": "cd client; npm start",
    "test": "npm run test-server && npm run test-client",
    "test-server": "jest",
    "test-server-w": "jest --watch",
    "test-client": "cd client; npm test",
    "test-client-w": "cd client; npm run test-w",
    "build": "concurrently 'npm run build-server' 'npm run build-client'",
    "build-server": "babel -d server/build ./server -s",
    "build-client": "cd client; npm run build",
    "prepush": "docker build .",
    "lint": "eslint ."
  },
  "author": "Stephen Cresswell",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.0",
    "eslint": "^4.4.1",
    "eslint-config-imperative-es6": "^2.0.0",
    "eslint-plugin-react": "^7.2.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "boom": "^5.2.0",
    "chalk": "^2.1.0",
    "confabulous": "^1.5.3",
    "express-session": "^1.15.5",
    "hogan.js": "^3.0.2",
    "http-proxy-middleware": "^0.17.4",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "on-headers": "^1.0.1",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-github": "^1.1.0",
    "prepper": "^1.0.0",
    "systemic": "^1.3.2",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.1.0"
  }
}
